{
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "page_action": {
    "default_icon": {
      "19": "images/logo_19.png",
      "38": "images/logo_38.png"
    },
    "default_title": "Mark all posts which are not graded as unread"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "https://is.muni.cz/"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "name": "[ONLINE_A] Not Graded Posts",
  "version": "0.96.2",
  "description": "Adds a page action to mark all forum posts which are not graded as unread.",
  "icons": {"128": "images/logo_128.png"}
}